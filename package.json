{
  "name": "webpack-mpa-example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build-client": "webpack --config client/webpack.config.js",
    "build-server": "babel server/src -d server/dist",
    "prestart": "npm run build-client",
    "start": "pm2 start pm2-config.json",
    "production": "NODE_ENV=production npm run build-client; npm run build-server; pm2 start pm2-config.json --env production",
    "clean": "rm -rf client/.build"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "preact": "^8.2.5",
    "preact-render-to-string": "^3.7.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0"
  }
